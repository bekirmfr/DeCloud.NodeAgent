{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DeCloud": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5100"
      }
    }
  },
  "Libvirt": {
    "VmStoragePath": "/var/lib/decloud/vms",
    "ImageCachePath": "/var/lib/decloud/images",
    "LibvirtUri": "qemu:///system",
    "VncPortStart": 5900
  },
  "Images": {
    "CachePath": "/var/lib/decloud/images",
    "VmStoragePath": "/var/lib/decloud/vms",
    "DownloadTimeout": "00:30:00"
  },
  "WireGuard": {
    "InterfaceName": "wg0",
    "ConfigPath": "/etc/wireguard",
    "ListenPort": 51820,
    "PrivateKeyPath": "/etc/wireguard/private.key",
    "PublicKeyPath": "/etc/wireguard/public.key",
    "Address": ""
  },
  "Heartbeat": {
    "Interval": "00:00:15",
    "OrchestratorUrl": "https://api.decloud.network"
  },
  "CommandProcessor": {
    "PollInterval": "00:00:05"
  },
  "Orchestrator": {
    "BaseUrl": "https://api.decloud.network",
    "ApiKey": "",
    "Timeout": "00:00:30"
  }
}
